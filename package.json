{
  "name": "papermark",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "ts": "turbo run ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "eslint": "^8.48.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.0",
    "prettier": "3.0.3",
    "turbo": "^1.10.15"
  },
  "packageManager": "pnpm@8.8.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
